{
  "[fish]": {
    "editor.defaultFormatter": "bmalehorn.vscode-fish"
  },
  "[toml]": {
    "editor.defaultFormatter": "be5invis.toml"
  },
  "editor.accessibilitySupport": "off",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Input Mono",
  "editor.fontSize": 15,
  "editor.inlineSuggest.enabled": true,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.rulers": [120],
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "explorer.confirmDelete": false,
  "files.associations": {
    "*.gotmpl": "go-template",
    "*.fish": "fish"
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 100,
  "files.trimTrailingWhitespace": true,
  "go.lintTool": "golangci-lint",
  "go.toolsManagement.autoUpdate": true,
  "gopls": {
    "experimentalWorkspaceModule": true
  },
  "python.terminal.executeInFileDir": true,
  "redhat.telemetry.enabled": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.fontFamily": "'Input Mono', 'Hack Nerd Font Mono'",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "args": ["-l"],
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh",
      "args": ["-l"]
    },
    "fish": {
      "path": "fish",
      "args": ["-l"]
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "workbench.colorCustomizations": {
    "[*Dark*]": {
      "sideBar.foreground": "#ca8dfd",
      "list.hoverForeground": "#00ff00"
    },
    "[*Light*]": {
      "sideBar.foreground": "#9627f2",
      "list.hoverForeground": "#f70707"
    }
  },
  "workbench.colorTheme": "Dracula",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.startupEditor": "none",
  "editor.wordWrapColumn": 120,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "terminal.integrated.env.osx": {
    "Q_NEW_SESSION": "1",
    "PATH": ""
  },
  "workbench.settings.applyToAllProfiles": [

  ],
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyleInactive": "underline"
}
