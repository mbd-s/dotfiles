{
  "editor.wordWrap": "on",
  "editor.rulers": [100],
  "terminal.integrated.fontFamily": "Input Mono",
  "workbench.colorCustomizations": {
    "[*Dark*]": {
      "sideBar.foreground": "#ca8dfd",
      "list.hoverForeground": "#00ff00"
    },
    "[*Light*]": {
      "sideBar.foreground": "#9627f2",
      "list.hoverForeground": "#f70707"
    }
  },
  "editor.fontFamily": "Input Mono",
  "editor.fontSize": 15,
  "terminal.integrated.fontSize": 16,
  "editor.wordWrapColumn": 120,
  "ruby.intellisense": "rubyLocate",
  "gopls": {
    "experimentalWorkspaceModule": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.autoSaveDelay": 100,
  "redhat.telemetry.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "terminal.external.osxExec": "iTerm.app",
  "go.toolsManagement.autoUpdate": true,
  "python.terminal.executeInFileDir": true,
  "editor.accessibilitySupport": "off",
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "go.lintTool": "golangci-lint",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "files.autoSave": "afterDelay",
  "files.associations": {
    "*.gotmpl": "go-template",
    "*.fish": "fish"
  },
  "explorer.confirmDelete": false,
  "workbench.preferredDarkColorTheme": "Dark-Dracula",
  "workbench.preferredLightColorTheme": "Atom One Light",
  "window.autoDetectColorScheme": true,
  "workbench.startupEditor": "none",
  "files.insertFinalNewline": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": false
  },
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "args": ["-l"],
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh",
      "args": ["-l"]
    },
    "fish": {
      "path": "fish",
      "args": ["-l"]
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "terminal.integrated.defaultProfile.osx": "fish",
  "[fish]": {
    "editor.defaultFormatter": "bmalehorn.vscode-fish"
  }
}
